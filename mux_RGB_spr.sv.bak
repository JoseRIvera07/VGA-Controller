module mux_RGB_spr  ( ,
	 input [4:0] selector,
	 output  logic [23:0] mux_out
);
/*
COLORES

'0xff48e0f0', 10101, 100100001110000011110000
'0xff181068', 10110, 110000001000000011010000
'0xff1038f8'  10111, 100000001110000011111000
]
*/
always_comb
begin
	case (selector)
		5'b00000: mux_out = 24'b000000000000000000000000;
		5'b00001: mux_out = 24'b101100001010000011000000;
		5'b00010: mux_out = 24'b111100001001000010000000;
		5'b00011: mux_out = 24'b111010001110000011100000;
		5'b00100: mux_out = 24'b111000001100000010000000;
		5'b00101: mux_out = 24'b110000001100000011000000;
		5'b00110: mux_out = 24'b100000001100000011010000;
		5'b00111: mux_out = 24'b111110001111100011111000;
		5'b01000: mux_out = 24'b100000001111000011011000;
		5'b01001: mux_out = 24'b100000001011000011111000;
		5'b01010: mux_out = 24'b110000001000000011000000;
		5'b01011: mux_out = 24'b110100001001000011101000;
		5'b01100: mux_out = 24'b100110001111000011101000;
		5'b01101: mux_out = 24'b101000001000000011001000;
		5'b01110: mux_out = 24'b101000001010000010010000;
		5'b01111: mux_out = 24'b100000001110000011010000;
		5'b10000: mux_out = 24'b101010001011000010010000;
		5'b10001: mux_out = 24'b100000001100000010010000;
		5'b10010: mux_out = 24'b100000001000000010010000;
		5'b10100: mux_out = 24'b101000001111100011111000;
		5'b10101: mux_out = 24'b100100001110000011110000;
		5'b10110: mux_out = 24'b110000001000000011010000;
		5'b10111: mux_out = 24'b100000001110000011111000;
		default:  mux_out = 24'b111111111111111111111111;
		
	endcase

end
endmodule